<% if current_user %>
  <h2> This is the homepage for <%= current_user.username %>!
<% end %>

<h2>See our peeps!</h2>

<ul id="peeps">

  <% @show_peeps.each do |show_peep| %>
    <li>
      Peep: <%= show_peep.peep %>
      Posted at: <%= show_peep.created_at %>
      By: <%= show_peep.user.username %>
    </li>
  <% end %>
</ul>
